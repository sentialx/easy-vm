{
  "name": "easy-vm",
  "version": "1.3.0",
  "description": "Run securely untrusted code with whitelisted Node modules",
  "main": "build/index",
  "types": "build/index.d.ts",
  "scripts": {
    "watch": "tsc --watch",
    "build": "tsc",
    "lint": "tslint \"src/**/*.ts*\"",
    "lint-fix": "prettier --write \"src/**/*.ts*\" && tslint \"src/**/*.ts*\" --fix",
    "prepublishOnly": "npm run build",
    "test": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sentialx/easy-vm.git"
  },
  "keywords": [
    "vm",
    "nodejs"
  ],
  "author": "sentialx",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sentialx/easy-vm/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/sentialx/easy-vm#readme",
  "devDependencies": {
    "@types/node": "12.7.4",
    "tslint": "5.19.0",
    "tslint-config-airbnb": "5.11.1",
    "tslint-config-prettier": "1.18.0",
    "prettier": "1.18.2"
  },
  "dependencies": {
    "ts-loader": "9.4.2",
    "typescript": "3.6.2"
  }
}